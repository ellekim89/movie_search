<h1> FAVORITES PAGE </h1>

<ul>
  <% myFavorites.forEach(function(fave){ %>
  <div class="well">
  <li><a href="/movies/show/<%= fave.imdbid %>">Title: <%= fave.title%></a><br>
  Year: <%= fave.year %><br>
  imdb Id: <%= fave.imdbid %><br>
  Poster: <%= fave.poster %><br><br>
  <a href="/favorites/<%= fave.id %>/comments">
   <button type="submit" class="btn btn-success">Add a comment!</button>
   </a>

   <a href="favorites/<%= fave.id %>/tags/new">
   <button type="submit" class="btn btn-default">Add a Tag</button>
   </a>
  </li>
     </div>
  <% }); %>
</ul>



